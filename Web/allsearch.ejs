<script>
    $('.close').click(function () {
        $('#ealert').hide();
    });
    $("#srch").autocomplete({
        source: drugs,
        treshold: 1,
        maximumItems: 100,
        onSelectItem: function () {
            $('#srch').focus();
        },
    });
    $("#categ").change(function () {
        let tmp = $(this).val();
        if (tmp == 'Drug-Disease') {
            $("#srch").autocomplete({
                source: ddpairs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Diseases') {
            $("#srch").autocomplete({
                source: diseases,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Drugs') {
            $("#srch").autocomplete({
                source: drugs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Genes') {
            $("#srch").autocomplete({
                source: genes,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'miRNAs') {
            $("#srch").autocomplete({
                source: mirnas,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'lncRNAs') {
            $("#srch").autocomplete({
                source: lncrnas,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'PDBs') {
            $("#srch").autocomplete({
                source: pdbs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Disease-Gene') {
            $("#srch").autocomplete({
                source: dgpairs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Disease-miRNA') {
            $("#srch").autocomplete({
                source: dmpairs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Disease-lncRNA') {
            $("#srch").autocomplete({
                source: dlpairs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        } else if (tmp == 'Drug-PDB') {
            $("#srch").autocomplete({
                source: dppairs,
                treshold: 1,
                maximumItems: 100,
                onSelectItem: function () {
                    $('#srch').focus();
                },
            });
        }
    });
    $('#srch').keypress(function (event) {

        var keycode = (event.keyCode ? event.keyCode : event.which);
        if (keycode == '13') {
            submitSearch();
        }

    });

    function submitSearch() {
        var tmp1 = {};
        tmp1.categ = $('#categ').val();
        tmp1.search = $('#srch').val();
        if (tmp1.search.length === 0)
            $('#ealert').show();
        else {
            document.querySelector('#obje').value = JSON.stringify(tmp1);
            document.querySelector('#finalSub').submit();
        }
    }
    $('#search-term').text(categ);
    $('#search-term2').text(sResults.length);
    let cnt = {};
    for(let i=97;i<=123;i++){
        cnt[`l${i}`] = 0;
    }
    for(let i=0;i<sResults.length;i++){
        let tmp = sResults[i].toLowerCase().charCodeAt(0);
        let htm = `<a href="/${categ}/${sResults[i]}">${sResults[i]}</a>`;
        if(tmp>=97 && tmp<=122) {
            addElement(`let${String.fromCharCode(tmp).toLowerCase()}`, 'li', "list-group-item", htm);
            cnt[`l${tmp}`]++;
        }else{
            addElement(`let1`,'li',"list-group-item",htm);
            cnt[`l123`]++;
        }
    }
    console.log(cnt);
    for(let i=97;i<=122;i++){
        if(cnt[`l${i}`] === 0)
            $(`#filter-bar${String.fromCharCode(i)}`).hide();
    }
    if(cnt[`l123`] === 0)
        $(`#filter-bar1`).hide();

    function addElement(parentID, elementTag, cls, html) {
        var p = document.getElementById(parentID);
        var newElement = document.createElement(elementTag);
        newElement.setAttribute('class', cls);
        newElement.innerHTML = html;
        p.appendChild(newElement);
    }
</script>
</body>
