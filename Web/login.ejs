<script>
    $('.close').click(function () {
        $('#ealert').hide();
    });
    $("#drug-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#disease-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#dd-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#gene-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#mirna-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#lncrna-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#se-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });
    $("#p-facet").hover(function(){
        $(this).css("background-color", "yellow");
        $(this).css('cursor', 'pointer');
    }, function(){
        $(this).css("background-color", "");
    });

    $("#srch").autocomplete({
        source: drugs,
        treshold:1,
        maximumItems:100,
        onSelectItem:function () {
            $('#srch').focus();
        },
    });
    $("#categ").change(function () {
        let tmp = $(this).val();
        if(tmp == 'Drug-Disease'){
            $("#srch").autocomplete({
                source: ddpairs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Diseases'){
            $("#srch").autocomplete({
                source: diseases,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Drugs'){
            $("#srch").autocomplete({
                source: drugs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Genes'){
            $("#srch").autocomplete({
                source: genes,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'miRNAs'){
            $("#srch").autocomplete({
                source: mirnas,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'lncRNAs'){
            $("#srch").autocomplete({
                source: lncrnas,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'PDBs'){
            $("#srch").autocomplete({
                source: pdbs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Disease-Gene'){
            $("#srch").autocomplete({
                source: dgpairs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Disease-miRNA'){
            $("#srch").autocomplete({
                source: dmpairs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Disease-lncRNA'){
            $("#srch").autocomplete({
                source: dlpairs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Drug-PDB'){
            $("#srch").autocomplete({
                source: dppairs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }
    });
    $('#srch').keypress(function(event){

        var keycode = (event.keyCode ? event.keyCode : event.which);
        if(keycode == '13'){
            submitSearch();
        }

    });
    function submitSearch() {
        var tmp1 = {};
        tmp1.categ = $('#categ').val();
        tmp1.search = $('#srch').val();
        if(tmp1.search.length === 0)
            $('#ealert').show();
        else{
            document.querySelector('#obje').value = JSON.stringify(tmp1);
            document.querySelector('#finalSub').submit();
        }
    }

    //new Functionality
    let gquery,gquery2,doid = '',dbankid = '';
    if(categ === 'Diseases'){
        gquery2 = query;
        if(sResults.length !== 0)
            doid = sResults[0].DOID;
        else
            doid = null;
        $("#search-term").text(query);
        if(doid)
            $("#search-link").attr("href", `https://disease-ontology.org/term/${doid}`);
        else{
            $("#search-link").attr("href", `#`);
            $("#search-link").removeAttr("target");
        }
        $(`#drug-facet`).addClass('d-flex');
        $(`#drug-facet`).show();
        $(`#drug-facet`).children('span').text(sResults.length);
        $(`#gene-facet`).addClass('d-flex');
        $(`#gene-facet`).show();
        $(`#gene-facet`).children('span').text(gResults.length);
        $(`#pos-info`).addClass('d-flex');
        $(`#pos-info`).children('span').text(posStats['Positive']);
        $(`#neg-info`).addClass('d-flex');
        $(`#neg-info`).children('span').text(posStats['Negative']);
        $(`#mirna-facet`).addClass('d-flex');
        $(`#mirna-facet`).show();
        $(`#mirna-facet`).children('span').text(miResults.length);
        $(`#lncrna-facet`).addClass('d-flex');
        $(`#lncrna-facet`).show();
        $(`#lncrna-facet`).children('span').text(lncResults.length);
        $(`#positivity`).show();
    }else if(categ === 'Drugs'){
        gquery = query;
        if(sResults.length !== 0)
            dbankid = sResults[0].Dbankid;
        else
            dbankid = null;
        $("#search-term").text(query);
        if(dbankid)
            $("#search-link").attr("href", `https://www.drugbank.ca/drugs/${dbankid}`);
        else{
            $("#search-link").attr("href", `#`);
            $("#search-link").removeAttr("target");
        }
        $(`#disease-facet`).addClass('d-flex');
        $(`#disease-facet`).show();
        $(`#disease-facet`).children('span').text(sResults.length);
        $(`#se-facet`).addClass('d-flex');
        $(`#se-facet`).show();
        $(`#se-facet`).children('span').text(seResults.length);
        $(`#pos-info`).addClass('d-flex');
        $(`#pos-info`).children('span').text(posStats['Positive']);
        $(`#neg-info`).addClass('d-flex');
        $(`#neg-info`).children('span').text(posStats['Negative']);
        $(`#p-facet`).show();
        $(`#p-facet`).addClass('d-flex');
        $(`#p-facet`).children('span').text(pResults.length);
        $(`#positivity`).show();
    }else if(categ === 'Drug-Disease'){
        gquery = query.split('+')[1];
        gquery2 = query.split('+')[0];
        if(sResults.length !== 0){
            dbankid = sResults[0].Dbankid;
            doid = sResults[0].DOID;
        }else{
            dbankid = null;
            doid = null;
        }
        $("#search-term").text(query.split('+')[0].replace(/^\w/, c => c.toUpperCase()));
        $("#search-term2").text(`- ${query.split('+')[1].replace(/^\w/, c => c.toUpperCase())}`);
        $("#sid2").show();
        if(doid)
            $("#search-link").attr("href", ` https://disease-ontology.org/term/${doid}`);
        else{
            $("#search-link").attr("href", `#`);
            $("#search-link").removeAttr("target");
        }
        if(dbankid)
            $("#search-link2").attr("href", `https://www.drugbank.ca/drugs/${dbankid}`);
        else{
            $("#search-link2").attr("href", `#`);
            $("#search-link2").removeAttr("target");
        }
        $(`#dd-facet`).show();
        $(`#dd-facet`).addClass('d-flex');
        $(`#dd-facet`).children('span').text(sResults.length);
        $(`#gene-facet`).show();
        $(`#gene-facet`).addClass('d-flex');
        $(`#gene-facet`).children('span').text(gResults.length);
        $(`#mirna-facet`).addClass('d-flex');
        $(`#mirna-facet`).show();
        $(`#mirna-facet`).children('span').text(miResults.length);
        $(`#lncrna-facet`).addClass('d-flex');
        $(`#lncrna-facet`).show();
        $(`#lncrna-facet`).children('span').text(lncResults.length);
        $(`#se-facet`).show();
        $(`#se-facet`).addClass('d-flex');
        $(`#se-facet`).children('span').text(seResults.length);
        $(`#p-facet`).show();
        $(`#p-facet`).addClass('d-flex');
        $(`#p-facet`).children('span').text(pResults.length);
    }

    for(let i=0; i<sResults.length; i++){
        let htm;
        if(categ === 'Diseases')
            htm = createHtml(i,sResults[i]['Drug'],sResults[i]['Label'],sResults[i]['Conf'],sResults[i]['Status'],sResults[i]['Dbankid']);
        else if(categ === 'Drugs')
            htm = createHtml(i,sResults[i]['Disease'],sResults[i]['Label'],sResults[i]['Conf'],sResults[i]['Status'],sResults[i]['DOID']);
        else if(categ === 'Drug-Disease')
            htm = createHtml(i,sResults[i]['Disease']+'+'+sResults[i]['Drug'],sResults[i]['Label'],sResults[i]['Conf'],sResults[i]['Status']);
        addElement(`resultQuery`, 'table', `result${i}`,`table table-sm table-borderless table-light shadow p-3 mb-2 rounded-lg` , htm);
        $(`#showmore${i}`).click(function () {
            let elem = $(`#descriptionCell${i}`).children("p.initial").children("span.more").children("span.mpart");
            if($(this).text().includes('more')){
                $(`#moredots${i}`).hide();
                elem.show();
                $(this).text(`Show less <`);
            }else if($(this).text().includes('less')){
                $(`#moredots${i}`).show();
                elem.hide();
                $(this).text(`Show more >`);
            }
        });
        if(sResults[i]['desc'] === ''){
            $(`#showmore${i}`).hide();
            $(`#moredots${i}`).hide();
        }
    }

    if(categ === 'Drugs' || categ === 'Drug-Disease'){
        for(let i=0; i<seResults.length; i++){
            let htm = createHtml2(i,seResults[i]);
            addElement(`resultQuery2`, 'table', `res${i}`,`table table-sm table-borderless table-light shadow p-3 mb-2 rounded` , htm);
        }
        for(let i=0; i<pResults.length; i++){
            let htm = createHtml4(i, pResults[i]);
            addElement(`resultQuery4`, 'table', `resp${i}`,`table table-sm table-borderless table-light shadow p-2 mb-2 rounded` , htm);
        }
    }
    if(categ === 'Diseases' || categ === 'Drug-Disease'){
        for(let i=0; i<gResults.length; i++){
            let htm = createHtml3(i, gResults[i]['Type'], gResults[i]['Conf'], gResults[i]['Gene'], 'g');
            addElement(`resultQuery3`, 'table', `resg${i}`,`table table-sm table-borderless table-light shadow p-3 mb-2 rounded` , htm);
        }
        for(let i=0; i<miResults.length; i++){
            let htm = createHtml3(i, miResults[i]['Type'], miResults[i]['Conf'], miResults[i]['Gene'], 'm');
            addElement(`resultQuery5`, 'table', `resm${i}`,`table table-sm table-borderless table-light shadow p-3 mb-2 rounded` , htm);
        }
        for(let i=0; i<lncResults.length; i++){
            let htm = createHtml3(i, lncResults[i]['Type'], lncResults[i]['Conf'], lncResults[i]['Gene'], 'l');
            addElement(`resultQuery6`, 'table', `resl${i}`,`table table-sm table-borderless table-light shadow p-3 mb-2 rounded` , htm);
        }
    }

    $(`#drug-facet`).click(function () {
        $(`#resultQuery`).show();
        $(`#resultQuery2`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery4`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery6`).hide();
    });
    $(`#disease-facet`).click(function () {
        $(`#resultQuery`).show();
        $(`#resultQuery2`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery4`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery6`).hide();
        $(`#positivity`).show();
    });
    $(`#dd-facet`).click(function () {
        $(`#resultQuery`).show();
        $(`#resultQuery2`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery4`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery6`).hide();
    });
    $(`#gene-facet`).click(function () {
        $(`#resultQuery3`).show();
        $(`#resultQuery`).hide();
        $(`#resultQuery2`).hide();
        $(`#resultQuery4`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery6`).hide();
    });
    $(`#se-facet`).click(function () {
        $(`#resultQuery2`).show();
        $(`#resultQuery`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery4`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery6`).hide();
        $(`#positivity`).hide();
    });
    $(`#p-facet`).click(function () {
        $(`#resultQuery4`).show();
        $(`#resultQuery`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery2`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery6`).hide();
    });
    $(`#mirna-facet`).click(function () {
        $(`#resultQuery5`).show();
        $(`#resultQuery`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery2`).hide();
        $(`#resultQuery4`).hide();
        $(`#resultQuery6`).hide();
    });
    $(`#lncrna-facet`).click(function () {
        $(`#resultQuery6`).show();
        $(`#resultQuery`).hide();
        $(`#resultQuery3`).hide();
        $(`#resultQuery2`).hide();
        $(`#resultQuery5`).hide();
        $(`#resultQuery4`).hide();
    });

    function createHtml(id,title,label,conf,status,link) {
        let tmp,tmp2,text,lnk,text2='',scateg,tget = `target="_blank" `;
        if(categ === 'Diseases'){
            scateg = 'Drugs';
            text = 'External Link';
            if(link)
                lnk = `https://www.drugbank.ca/drugs/${link}`;
            else{
                lnk = '#';
                tget = '';
            }
        }else if(categ === 'Drugs'){
            scateg = 'Diseases';
            text = 'External Link';
            if(link)
                lnk = `https://disease-ontology.org/term/${link}`;
            else{
                lnk = '#';
                tget = '';
            }
        }else{
            scateg = 'Drug-Disease';
            text2 = ' d-none';
        }
        conf2 = conf*100;
        if(conf2 >= 70.0)
            tmp = 'badge-success';
        else if(conf2 <= 40.0)
            tmp = 'badge-danger';
        else
            tmp = 'badge-warning';
        conf2 = conf2.toFixed(2);
        if(label === 'Positive')
            tmp2 = 'badge-success';
        else if(label === 'Negative')
            tmp2 = 'badge-danger';
        let htm = `<tbody>
                    <tr style="background-color: #e3faf0">
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 94%"><h3 id="titleCell${id}"><span style="background-color:yellowgreen" class="badge badge-info rounded-circle"> D </span>&nbsp;&nbsp;
                                <a href="/${categ}/${query}/dd=${title}">${title}</a>&nbsp;&nbsp;<span class="badge ${tmp2} label">${label}</span>&nbsp;&nbsp;<a class="btn btn-info float-right" href="/${scateg}/${title}">Query Link</a></h3>
                        </td>
                        <td rowspan="1" style="width: 3%"></td>
                    </tr>
                    <tr>
                        <td rowspan="1" style="width: 3%"></td>
                        <td style="width: 88%" id="descriptionCell${id}">
                            <p class="initial"><span class="fpart"><a ${tget}class="btn btn-outline-primary${text2}" href="${lnk}">${text}</a></span><span id="moredots${id}">...&nbsp;</span><span class="more"><span class="mpart" style="display: none">!</span>&nbsp;&nbsp;
                                    <a id="showmore${id}" href="javascript:void(0)">Show more &gt;</a></span>
                            </p>
                            <p class="lead" id="statsParagraph${id}">Confidence Score <span
                                        class="badge ${tmp} conf">${conf2}%</span>&nbsp;&nbsp;&nbsp;Status
                                <span class="badge badge-danger stats">${status}</span>
                            </p>
                        </td>
                        <td rowspan="1" style="width: 3%"></td>
                    </tr>
                </tbody>`;
        return htm;
    }

    function createHtml2(id,title) {
        let htm = `<tbody>
                    <tr style="background-color: #fcf5de">
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 94%"><h3 id="tCellse${id}"><span style="background-color:#ffac1b" class="badge badge-info rounded-circle"> E </span>&nbsp;&nbsp;
                                <a href="#">${title}</a>&nbsp;&nbsp;</h3>
                        </td>
                        <td rowspan="1" style="width: 3%;"></td>
                    </tr>
                    <tr>
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 88%" id="dCellse${id}">
                            <p class="initial" style="display: none">Dummy Initital Desc</p>
                        </td>
                        <td rowspan="1" style="width: 3%;"></td>
                    </tr>
                </tbody>`;
        return htm;
    }
    function createHtml4(id,title) {
        let htm = `<tbody>
                    <tr style="background-color: #dcecf7">
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 94%"><h3 id="tCellp${id}"><span style="background-color:dodgerblue" class="badge badge-info rounded-circle"> P </span>&nbsp;&nbsp;
                                <a href="/pdb/${title}/d=${gquery}">${title}</a>&nbsp;&nbsp;</h3>
                        </td>
                        <td rowspan="1" style="width: 3%;"></td>
                    </tr>
                    <tr style="display: none">
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 88%" id="dCellp${id}">
                            <p class="initial">Dummy Initital Desc</p>
                        </td>
                        <td rowspan="1" style="width: 3%;"></td>
                    </tr>
                </tbody>`;
        return htm;
    }

    function createHtml3(id,label,conf,title,gtype) {
        let tmp,type;
        if(gtype === 'g')
            type = 'gene';
        else if(gtype === 'm')
            type = 'miRNA';
        else if(gtype === 'l')
            type = 'lncRNA';
        if(conf === 'Verified')
            tmp = 'badge-success';
        else if(conf === 'Medium')
            tmp = 'badge-warning';
        else
            tmp = 'badge-danger';
        let htm = `<tbody>
                    <tr style="background-color: #f5ebff">
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 94%"><h3 id="tCell${gtype}${id}"><span style="background-color:darkmagenta" class="badge badge-info rounded-circle"> ${gtype.toUpperCase()} </span>&nbsp;&nbsp;
                                <a href="/${type}/${title}/d=${gquery2}">${title}</a>&nbsp;&nbsp;&nbsp;<span class="badge badge-info label">${label}</span>&nbsp;</h3>
                        </td>
                        <td rowspan="1" style="width: 3%;"></td>
                    </tr>
                    <tr>
                        <td rowspan="1" style="width: 3%;"></td>
                        <td style="width: 88%" id="dCell${gtype}${id}">
                            <p class="initial" style="display: none">Dummy Initital Desc</p>
                            <p class="lead" id="statsVerify${id}">Confidence Score <span
                                        class="badge ${tmp} conf">${conf}</span>&nbsp;&nbsp;&nbsp;</span>
                            </p>
                        </td>
                        <td rowspan="1" style="width: 3%;"></td>
                    </tr>
                </tbody>`;
        return htm;
    }

    function addElement(parentID, elementTag, elementID, cls, html) {
        var p = document.getElementById(parentID);
        var newElement = document.createElement(elementTag);
        newElement.setAttribute('id', elementID);
        newElement.setAttribute('class', cls);
        newElement.style.overflow = "hidden";
        newElement.innerHTML = html;
        p.appendChild(newElement);
    }
</script>

</body>
