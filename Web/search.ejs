<script>
    if(message === 'Error'){
        $('#ialert').show();
        message = '';
    }
    $('#close2').click(function () {
        $('#ealert').hide();
    });
    $('#close').click(function () {
        $('#ialert').hide();
    });
    $("#vcount-stat").text(`${vcount}`);
    $("#drug-stat").text(`${druglen}`);
    $("#disease-stat").text(`${dislen}`);
    $("#gene-stat").text(`${glen}`);
    $("#mirna-stat").text(`${milen}`);
    $("#lncrna-stat").text(`${lnclen}`);
    $("#pdb-stat").text(`${pdblen}`);
    $("#dd-stat").text(`${ddlen}`);
    $("#dg-stat").text(`${dglen}`);
    $("#dp-stat").text(`${dplen}`);
    $("#dm-stat").text(`${dmlen}`);
    $("#dl-stat").text(`${dllen}`);
    $("#srch").autocomplete({
        source: drugs,
        treshold:1,
        maximumItems:100,
        onSelectItem:function () {
            $('#srch').focus();
        },
    });
    $("#categ").change(function () {
        let tmp = $(this).val();
        if(tmp == 'Drug-Disease'){
            $("#srch").autocomplete({
                source: ddpairs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Diseases'){
            $("#srch").autocomplete({
                source: diseases,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Drugs'){
            $("#srch").autocomplete({
                source: drugs,
                treshold:1,
                maximumItems:100,
                onSelectItem:function () {
                    $('#srch').focus();
                },
            });
        }else if(tmp == 'Genes'){
            $("#srch").autocomplete({
                source: genes,
                treshold:1,
                maximumItems:100,
